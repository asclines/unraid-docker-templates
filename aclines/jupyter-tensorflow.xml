  <Icon>/plugins/dynamix.docker.manager/images/question.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DateInstalled>1538882463</DateInstalled>
  <Description>Jupyter Notebook Scientific Python Stack w/ Tensorflow </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8888</HostPort>
        <ContainerPort>8888</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/notebooks</HostDir>
      <ContainerDir>/home/jovyan/work </ContainerDir>
      <Mode>rw</Mode>
