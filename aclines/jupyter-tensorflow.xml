<?xml version="1.0"?>
<Container version="2">
  <Name>jupyter-tensorflow</Name>
  <Repository>jupyter/tensorflow-notebook</Repository>
  <Registry>https://hub.docker.com/r/jupyter/tensorflow-notebook/~/dockerfile/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/jupyter/tensorflow-notebook/</Support>
  <Overview>Jupyter Notebook Scientific Python Stack w/ Tensorflow </Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:8888]/</WebUI>
